<Window x:Class="MaMoOr_GUI.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:MaMoOr_GUI"
        mc:Ignorable="d"
        Title="Login" Height="450" Width="800">
    <Grid>
        <GroupBox Header="Login" HorizontalAlignment="Left" Height="152" Margin="255,130,0,0" VerticalAlignment="Top" Width="269">
            <StackPanel>
                <TextBox HorizontalAlignment="Left" Height="22" Margin="10,27,0,0" TextWrapping="Wrap" Text="username" VerticalAlignment="Top" Width="235"/>
                <TextBox HorizontalAlignment="Left" Height="22" Margin="10,10,0,0" TextWrapping="Wrap" Text="password" VerticalAlignment="Top" Width="235"/>
                <Button Content="Login" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" Width="75"/>
            </StackPanel>
        </GroupBox>
        

    </Grid>
</Window>
